generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}




model User {
  id       String  @id @default(uuid())
  name String  @unique
  email    String  @unique
  image    String  @default("/defaultProfile.png")
  password String?
  provider String @default("credentials")
}
